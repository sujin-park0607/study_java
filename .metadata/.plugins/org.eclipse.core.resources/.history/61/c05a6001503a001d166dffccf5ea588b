import java.util.*;

class Grade{
	int korean;
	int math;
	int english;
	
	public void setGrade(int korean,int math,int english) {
		this.korean = korean;
		this.math = math;
		this.english = english;
	}
	
	public void run() {
		String result;
		Scanner sc = new Scanner(System.in);
		System.out.print("국어: ");
		int korean = sc.nextInt();
		
		System.out.print("수학: ");
		int math = sc.nextInt();
		
		System.out.print("영어: ");
		int english = sc.nextInt();
		int avg = (korean + math + english) / 3;
		
		if(avg >= 90) {
			result = "수";
		}
		else if(avg >= 80) {
			result = "우";
		}
		else if(avg >= 70) {
			result = "미";
		}
		else if(avg >= 60) {
			result = "양";
		}
		else{
			result = "가";
		}
		
		System.out.println("당신의 등급은?!: "+ result);
	}
}

public class GradeTest {

	public static void main(String[] args) {
		Grade grade = new Grade();
        grade.run();
		
	}

}
