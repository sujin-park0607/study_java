package edu.lion.car;
import java.util.*;

public class Array {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		System.out.print("행, 열을 입력하여 주시오: ");
		int row = sc.nextInt();
		int column = sc.nextInt();
		
		int[][] arr = new int[5][5];
		
		arr[row][column] = 'x';
		
		System.out.println("  0  1  2  3  4");
		for(int i=0; i < 5; i++) { 
			for(int j=0; i < 5; j++) {
				System.out.print(i+" ");
				System.out.print(arr[i][j]+" ");
			}
			System.out.println();
				
		}

	}

}
