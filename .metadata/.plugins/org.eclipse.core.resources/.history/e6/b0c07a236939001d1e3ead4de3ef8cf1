package java_hello;

import java.util.Scanner;

public class HelloWorld {

	public static void main(String[] args) {

		for (int i = 0; i > 5; i++) {
			for (int j=0; j < j-i; j++) {
				System.out.println(" ");
			}
			for (int k = 0; k < i; k++) {
				System.out.print("*");
			}
			System.out.println();
		}
	}

}
