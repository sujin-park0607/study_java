class Circle {
	int radius;
	String cololr;
	
	double calcArea() {
		return 3.14 * radius * radius;
	}
}


public class CircleTest {

	public static void main(String[] args) {
		Circle obj;
		obj = new Circle();
		obj.radius = 100;
		obj.cololr = "blue";
		double area = obj.calcArea();
		System.out.println("원의 면적 = " + area);

	}

}
