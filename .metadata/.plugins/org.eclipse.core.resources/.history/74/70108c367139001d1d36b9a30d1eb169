package java_hello;

import java.util.Random;

public class HelloWorld {

	public static void main(String[] args) {
		Random ran = new Random();
		int[] arr = new int[10];
		
		int max = 0;
		
		for(int i=0; i< arr.length; i++) {
			arr[i] = ran.nextInt(100);
			System.out.print(arr[i]+ " ");
		}
		System.out.println();
		
		for (int i : arr) {
			if(i > max){
				max = i;
			}
		}
		
		System.out.println(max);
	}
}
