package java_hello;
import java.util.Scanner;

public class HelloWorld {

	public static void main(String[] args) {
		int num1 = 5;
		int num2 = 7;
		int result = 1; 
		
		while(true){
			result ++;
			
			if(result % num1 ==0 && result % num2 ==0 ) {
				System.out.println(result);
				break;
			}
		}
		
	}

}
