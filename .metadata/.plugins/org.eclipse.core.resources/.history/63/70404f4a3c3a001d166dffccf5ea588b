class Student{
	String name;
	int number;
	int age;
	
}


public class StudentTest {

	public static void main(String[] args) {
		Student obj = new Student();
		obj.name = "Kim";
		obj.number = 20180001;
		obj.age = 20;
		
		System.out.println("이름: " + obj.name);
		System.out.println("학번: " + obj.number);
		System.out.println("나이: " + obj.age);

	}

}
