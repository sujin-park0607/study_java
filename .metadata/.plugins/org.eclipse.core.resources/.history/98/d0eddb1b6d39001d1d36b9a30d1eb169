package java_hello;

public class Star {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for (int i=1; i <= num; i++) {
					
			for (int j=1; j <= 5-i; j++) {
				System.out.print(" ");
			}
			
			for (int k=1; k <= i; k++) {
				System.out.print("*");
			}
			
			System.out.println();
		}

	}

}
