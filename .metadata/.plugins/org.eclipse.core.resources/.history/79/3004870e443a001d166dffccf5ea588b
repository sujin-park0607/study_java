import java.util.*;

class Grade{
	int korean;
	int math;
	int english;
	
	public void setGrade(int korean,int math,int english) {
		this.korean = korean;
		this.math = math;
		this.english = english;
	}
	
	public  String getGrade() {
		int avg = (korean + math + english) / 3;
		
		if(avg >= 90) {
			return "수";
		}
		else if(avg >= 80) {
			return "우";
		}
		else if(avg >= 70) {
			return "미";
		}
		else if(avg >= 60) {
			return "양";
		}
		else{
			return "가";
		}
	}
}

public class GradeTest {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("국어: ");
		int korean = sc.nextInt();
		
		System.out.print("수학: ");
		int math = sc.nextInt();
		
		System.out.print("영어: ");
		int english = sc.nextInt();
		
		Grade grade = new Grade();
		grade.setGrade(korean, math, english);
		System.out.println(grade.getGrade());
		
		
	}

}
