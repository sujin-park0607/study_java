class Car{
	
	private int speed;
	
	public Car(int speed) {
		this.speed = speed;
	}
	
	public int getSpeed() {
		return speed;
	}
	
	public void setSpeed(int speed) {
		this.speed = speed;
	}
}

public class JvmTest {

	public static void main(String[] args) {
		
		Car car = new Car(30);
		System.out.println(car.getSpeed());

	}

}
