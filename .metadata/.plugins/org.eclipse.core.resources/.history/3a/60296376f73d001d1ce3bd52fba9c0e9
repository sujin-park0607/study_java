package java_0923;

import java.util.Arrays;

public class BubbleSort {

	public static void main(String[] args) {
		int[] a = {7, 4, 5, 1, 3};
		int temp;
		
		for(int i = 0; i < a.length; i++) {
			for(int j=i; j < a.length - 1; j++) {
				if(a[i] > a[i+1]) {
					temp = a[i];
					a[i] = a[i+1];
					a[i+1] = temp;
				}
				System.out.println(Arrays.toString(a));
				
			}
		}

	}

}
